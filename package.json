{
  "name": "stampver",
  "version": "1.0.4",
  "engines": {
    "node": ">=8.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "description": "Version stamping tool",
  "scripts": {
    "build": "babel src -d dist -s --ignore *.test.js",
    "release": "babel-node tasks/release.js",
    "debug:stampver": "babel-node --inspect-brk src/stampver.js",
    "test": "jest"
  },
  "bin": {
    "stampver": "dist/stampver-cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlyonsmith/stampver.git"
  },
  "keywords": [
    "version",
    "stamp",
    "tool"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlyonsmith/stampver/issues"
  },
  "homepage": "https://github.com/jlyonsmith/stampver#readme",
  "dependencies": {
    "chalk": "^2.1.0",
    "joi": "^11.1.1",
    "json5": "^0.5.1",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "xregexp": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "istanbul": "^0.4.5",
    "jest": "^21.1.0",
    "jest-babel-istanbul": "^0.1.0",
    "nodemon": "^1.12.1",
    "tmp": "0.0.33"
  }
}
