{
  "name": "@johnls/stampver",
  "version": "1.2.1",
  "engines": {
    "node": ">=10.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "description": "Version stamping tool",
  "scripts": {
    "build": "rm -rf dist/* && babel src -d dist -s --ignore **/*.test.js",
    "debug:stampver": "babel-node --inspect-brk src/stampver.js",
    "test": "jest $* --coverage",
    "deploy": "npm publish --access public"
  },
  "bin": {
    "stampver": "dist/stampver.js"
  },
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlyonsmith/stampver.git"
  },
  "keywords": [
    "version",
    "stamp",
    "tool"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlyonsmith/stampver/issues"
  },
  "homepage": "https://github.com/jlyonsmith/stampver#readme",
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "chalk": "^4.0.0",
    "joi": "^14.3.1",
    "json5": "^2.1.3",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.5",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "tmp-promise": "^3.0.2",
    "xregexp": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "jest-cli": "^26.0.1",
    "monzilla": "^1.1.2"
  }
}
