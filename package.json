{
  "name": "@johnls/stampver",
  "version": "1.2.0",
  "engines": {
    "node": ">=10.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "description": "Version stamping tool",
  "scripts": {
    "build": "rm -rf dist/* && babel src -d dist -s --ignore **/*.test.js",
    "debug:stampver": "babel-node --inspect-brk src/stampver.js",
    "test": "jest $* --coverage",
    "deploy": "npm publish --access public"
  },
  "bin": {
    "stampver": "dist/stampver.js"
  },
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlyonsmith/stampver.git"
  },
  "keywords": [
    "version",
    "stamp",
    "tool"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlyonsmith/stampver/issues"
  },
  "homepage": "https://github.com/jlyonsmith/stampver#readme",
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "chalk": "^2.4.2",
    "joi": "^14.3.1",
    "json5": "^2.1.0",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "tmp-promise": "^1.0.5",
    "xregexp": "^4.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "jest-cli": "^24.1.0",
    "monzilla": "^1.1.1"
  }
}
